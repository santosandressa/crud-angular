{
  "name": "crud-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.js",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "lint": "eslint . --fix",
    "prettier": "npx prettier --write .",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
